<h1 class='text-center'>Dustbin Stub</h1>
<div class="modal fade" id="myModal" role="dialog" style="padding:10px;">
    <div class="modal-dialog modal-md">
      <div class="modal-content">
        <div class="modal-header">
          <button type="button" class="close" data-dismiss="modal">&times;</button>
          <h4 class="modal-title">Select Location</h4>
        </div>
        <div class="modal-body">
        	    <div id="map" style="width: 100%; height: 300px;"></div>
        	    <form>
        	    	<div class="form-group">
        	    		Lat: <input type="text" class="form-control" id="lat" placeholder="Latitude"\>
        	    		Long: <input type="text" class="form-control" id="long" placeholder="Longitude"\>
        	    	</div>
        	    </form>
        </div>
        <div class="modal-footer">
          <button type="button" class="btn btn-default" data-dismiss="modal" id="btnSave">Save</button>
          <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
        </div>
      </div>
    </div>
  </div>

<div class="container-fluid">
	<div class="row">
		<div class="col-md-2 col-md-offset-4 text-center">
			<button id="btnAdd" type="button" class="btn-success btn btn-lg mybtn">Add Bin</button>
		</div>
		<div class="col-md-2 text-center">
			<button id="btnRemove" type="button" class="btn-warning btn btn-lg mybtn">Remove Bin</button>
		</div>
	</div>
	<div class="row" style="margin-top:10px;">
		<div class="container-fluid">
			<div class="row">
				<div id='data'>
				</div>
			</div>
		</div>
	</div>
</div>
</div>
